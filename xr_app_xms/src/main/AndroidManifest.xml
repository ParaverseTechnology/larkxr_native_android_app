<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pxy.xr_app_xms">

    <!-- This is the platform API where NativeActivity was introduced. -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        >
        <!-- Our activity is the built-in NativeActivity framework class.
                 This will take care of integrating with our NDK code. -->
        <activity android:name=".SvrNativeActivity" android:label="@string/app_name" android:configChanges="orientation|keyboardHidden" android:screenOrientation="${orientation}">
            <!-- Tell NativeActivity the name of or .so -->
            <meta-data android:name="android.app.lib_name" android:value="lark_xr_xms" />
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="com.qti.intent.category.SNAPDRAGON_VR"/>
                <category android:name="com.ximmerse.category.WONDERLAND_AR"/>
            </intent-filter>
        </activity>
    </application>


</manifest>